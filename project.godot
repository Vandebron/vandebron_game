config_version=5

[application]

config/name="Vandebron"
run/main_scene="res://world/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0.2, 0.239216, 0.278431, 1)
boot_splash/image="res://export/assets/splash.png"
config/icon="res://icon.svg"

